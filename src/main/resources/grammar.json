{
  "grammar": {
    "Program": [["SubProgram"]],
    "SubProgram": [["begin", "DeclarationStmtList", ";", "ExecStmtList", ";", "end"]],

    "DeclarationStmtList": [["DeclarationStmt", "DeclarationStmtListPrime"]],
    "DeclarationStmtListPrime": [
      [";", "DeclarationStmt", "DeclarationStmtListPrime"],
      ["ε"]
    ],

    "DeclarationStmt": [
      ["VariableDecl"],
      ["FunctionDecl"]
    ],
    "VariableDecl": [["integer", "Variable"]],
    "Variable": [["Identifier"]],

    "Identifier": [["identifier"]],

    "FunctionDecl": [["integer", "function", "Identifier", "(", "Parameter", ")", ";", "FunctionBody"]],
    "Parameter": [["Variable"]],
    "FunctionBody": [["begin", "DeclarationStmtList", ";", "ExecStmtList", ";", "end"]],

    "ExecStmtList": [["ExecStmt", "ExecStmtListPrime"]],
    "ExecStmtListPrime": [
      [";", "ExecStmt", "ExecStmtListPrime"],
      ["ε"]
    ],

    "ExecStmt": [
      ["ReadStmt"],
      ["WriteStmt"],
      ["AssignStmt"],
      ["IfStmt"]
    ],
    "ReadStmt": [["read", "(", "Variable", ")"]],
    "WriteStmt": [["write", "(", "Variable", ")"]],
    "AssignStmt": [["Variable", ":=", "ArithExpr"]],

    "ArithExpr": [["Term", "ArithExprPrime"]],
    "ArithExprPrime": [
      ["+", "Term", "ArithExprPrime"],
      ["-", "Term", "ArithExprPrime"],
      ["ε"]
    ],

    "Term": [["Factor", "TermPrime"]],
    "TermPrime": [
      ["/", "Factor", "TermPrime"],
      ["*", "Factor", "TermPrime"],
      ["ε"]
    ],

    "Factor": [
      ["FuncCall"],
      ["Variable"],
      ["Constant"]
    ],
    "Constant": [["UnsignedInt"]],
    "UnsignedInt": [["constant"]],

    "IfStmt": [["if", "CondExpr", "then", "ExecStmt", ";", "else", "ExecStmt"]],
    "CondExpr": [["ArithExpr", "RelOp", "ArithExpr"]],
    "RelOp": [
      ["<"],
      ["<="],
      [">"],
      [">="],
      ["="],
      ["<>"]
    ],
    "FuncCall": [["Identifier", "(", "ArithExpr", ")"]]
  },
  "startSymbol": "Program",
  "terminals": [
    "begin", "end", "integer", "function", "read", "write", "if", "then", "else",
    "identifier",
    "constant",
    ";", "(", ")", ":=", "+", "-", "*", "/", "=",
    "<", "<=", ">", ">=", "<>",
    "EOF"
  ],
  "nonTerminals": [
    "Program", "SubProgram",
    "DeclarationStmtList", "DeclarationStmtListPrime", "DeclarationStmt",
    "VariableDecl", "Variable", "Identifier", "IdentifierTail",
    "FunctionDecl", "Parameter", "FunctionBody",
    "ExecStmtList", "ExecStmtListPrime", "ExecStmt",
    "ReadStmt", "WriteStmt", "AssignStmt",
    "ArithExpr", "ArithExprPrime", "Term", "TermPrime",
    "Factor", "Constant", "UnsignedInt", "UnsignedIntTail",
    "IfStmt", "CondExpr", "FuncCall", "RelOp"
  ]
}